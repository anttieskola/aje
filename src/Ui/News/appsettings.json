{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RedisConfiguration": {
    "Host": "localhost:6379"
  },
  "TranslateConfiguration": {
    "Host": "http://127.0.0.1:5888",
    "TimeoutInSeconds": 30
  },
  "FileSystemConfiguration": {
    "RootFolder": "/var/aje"
  },
  "LlamaConfiguration": {
    "LogFolder": "/var/aje/ai",
    "Servers": [
      {
        "ResourceName": "llama-zeus",
        "Host": "http://zeus",
        "TimeoutInSeconds": 1800,
        "MaxTokenCount": 16384
      },
      {
        "ResourceName": "llama-apollo",
        "Host": "http://apollo",
        "TimeoutInSeconds": 1800,
        "MaxTokenCount": 32768
      }
    ]
  }
}