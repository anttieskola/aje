{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http": "Warning"
    }
  },
  "ConnectionStrings": {
    "NewsAnalyzer": "Host=/run/postgresql;Database=newsanalyzer;"
  },
  "RedisConfiguration": {
    "Host": "apollo:6379"
  },
  "LlamaConfiguration": {
    "LogFolder": "/var/aje/ai",
    "Servers": [
      {
        "ResourceName": "llama-zeus",
        "Host": "http://zeus",
        "TimeoutInSeconds": 1800,
        "MaxTokenCount": 16384
      },
      {
        "ResourceName": "llama-apollo",
        "Host": "http://apollo",
        "TimeoutInSeconds": 1800,
        "MaxTokenCount": 32768
      }
    ]
  }
}