{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http": "Warning",
      "Microsoft.EntityFrameworkCore.Database": "Warning"
    }
  },
  "ConnectionStrings": {
    "NewsAnalyzer": "Host=/run/postgresql;Database=newsanalyzer;"
  },
  "RedisConfiguration": {
    "Host": "apollo:6379"
  },
  "TranslateConfiguration": {
    "Host": "http://127.0.0.1:5888",
    "TimeoutInSeconds": 30
  },
  "FileSystemConfiguration": {
    "RootFolder": "/var/aje"
  },
  "LlamaConfiguration": {
    "LogFolder": "/var/aje/ai",
    "Servers": [
      {
        "ResourceName": "zeus",
        "Host": "http://zeus",
        "TimeoutInSeconds": 360,
        "MaxTokenCount": 16384
      }
    ]
  }
}